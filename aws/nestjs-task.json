{
    "family": "nestjs-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "nestjs-container",
        "image": "${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_REGION}.amazonaws.com/nestjs-server:${VERSION}",
        "portMappings": [
          {
            "containerPort": 3000,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "environment": [],
        "secrets": [
          {
            "name": "ATLAS_URI",
            "valueFrom": "arn:aws:secretsmanager:${AWS_REGION}:${AWS_ACCOUNT_ID}:secret:ATLAS_URI"
          }
        ]
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/ecsTaskExecutionRole"
  }
  